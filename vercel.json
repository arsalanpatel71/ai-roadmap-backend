{
  "version": 2,
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.12",
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/ws/(.*)",
      "dest": "app/main.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "app/main.py"
    }
  ]
}
